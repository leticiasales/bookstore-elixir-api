{
  "AWSEBDockerrunVersion": 1,
  "volumes": [
    {
      "name": "api",
      "host": {
        "sourcePath": "/app"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "api",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 4000,
          "containerPort": 4000
        }
      ]
    }
  ]
}